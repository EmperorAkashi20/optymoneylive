<script>
   $( "#first-con" ).click(function() {
     $( ".first-show" ).hide();
     $( ".second-show" ).show();
     $( ".third-show" ).hide();
   });
   $( "#second-con" ).click(function() {
     $( ".first-show" ).hide();
     $( ".second-show" ).hide();
     $( ".third-show" ).show();
   });
   $( "#confirm" ).click(function() {
     $( ".first-show" ).hide();
     $( ".second-show" ).hide();
     $( ".third-show" ).show();
   });
</script>
<!-- ------------------------- SIP date validation in single page of wealth -------------------------------  -->
<script type="text/javascript">
   $("#sip").click(function(){
       
       var val =$("#sip_date").val();
       //alert(val);
       if (parseInt(val) == 0) 
       {
           event.preventDefault();
           alert("Please choose the date for SIP");
           $('#sip_date').focus();
       }   
       
   });
</script>
