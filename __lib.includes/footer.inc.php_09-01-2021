<script type="text/javascript">
    $(".formemail").focusout(function(){
        var EmailAddress  = /^([\.a-zA-Z0-9_\-])+@([a-zA-Z0-9_\-])+(([a-zA-Z0-9_\-])*\.([a-zA-Z0-9_\-])+)+$/;
        var val = $(this).val();
        if (val != "") {
            if(!val.match(EmailAddress)) {
                $("#formemail_error").text("Email Address not valid");   
            } else {
                $("#formemail_error").text("");   
            }
        } else {
            $("#formemail_error").text("Email is required");
        }
    });
</script>
<!-- -------------------------------------------------- Validation for name ------------------------------------------------------------- -->
<script type="text/javascript">
    $(".formname").focusout(function(){
        var letters       = /^[A-Za-z '.-]+$/;
        var val = $(this).val();
        if (val != "") {
            if(!val.match(letters)) {
                $("#formname_error").text("Name must contain only letters, space");
            } else {
                $("#formname_error").text("");
            }
        } else {
            $("#formname_error").text("Name is required");
        }
    });
</script>
<!-- -------------------------------------------------- Validation for number ----------------------------------------------------------- -->
<script type="text/javascript">
   $(".formnumber").focusout(function(){
       //alert("EMAIL INSERT");
       var letters       = /^0{0,1}[1-9]{1}[0-9]{1,2}[\s]{0,1}[\-]{0,1}[\s]{0,1}[1-9]{1}[0-9]{6}$/;
       var val = $(this).val();
       if (val != "") {
           if(!val.match(letters)) {
                $("#formnumber_error").text("Enter Valid Phone Number");
           } else {
            $("#formnumber_error").text("");
           }
       }
       else {
            $("#formnumber_error").text("Phone number is required");
       }
   });
</script>
<script type="text/javascript">
// Diwali POpup poster
$(document).ready(function() {  
    var id = '#dialog';
    //Get the screen height and width
    var maskHeight = $(document).height();
    var maskWidth = $(window).width();

    //Set heigth and width to mask to fill up the whole screen
    $('#mask').css({'width':maskWidth,'height':maskHeight});

    //transition effect   
    $('#mask').fadeIn(500); 
    $('#mask').fadeTo("slow",0.9);  

    //Get the window height and width
    var winH = $(window).height();
    var winW = $(window).width();
      
    //Set the popup window to center
    $(id).css('top',  winH/2-$(id).height()/2);
    $(id).css('left', winW/2-$(id).width()/2);

    //transition effect
    $(id).fadeIn(2000);   

    //if close button is clicked
    $('.window .close').click(function (e) {
        //Cancel the link behavior
        e.preventDefault();

        $('#mask').hide();
        $('.window').hide();
    });   

    //if mask is clicked
    $('#mask').click(function () {
        $(this).hide();
        $('.window').hide();
    });   

});

</script>
<script type="text/javascript">
    $("#eca_name").focusout(function(){
        var letters = /^[A-Za-z '.-]+$/;
        var val = $(this).val();
        if (val != "") {
            if(!val.match(letters)) {
                $("#eca_name_error").text("Name must contain only letters, space");   
            }
            else {
                $("#eca_name_error").text("");   
            }
        } else {
            $("#eca_name_error").text("Name is required");
        }
    });
    $("#eca_email").focusout(function(){
        var letters = /^([\.a-zA-Z0-9_\-])+@([a-zA-Z0-9_\-])+(([a-zA-Z0-9_\-])*\.([a-zA-Z0-9_\-])+)+$/;
        var val = $(this).val();
        if (val != "") {
            if(!val.match(letters)) {
                $("#eca_email_error").text("Email Address not valid");   
            } else {
                $("#eca_email_error").text("");   
            }
        } else {
            $("#eca_email_error").text("Email is required");
        }
    });
   $("#eca_mob").focusout(function(){
        var letters = /^(?:(?:\+|0{0,2})91(\s*[\-]\s*)?|[0]?)?[6789]\d{9}$/;
       var val = $(this).val();
       if (val != "") {
           if(!val.match(letters)) {
               $("#eca_mob_error").text("Phone number must contain only numbers and 10 digits");   
           } else {
            $("#eca_mob_error").text("");   
           }
       } else {
            $("#eca_mob_error").text("Phone number is required");
       }
   });
   $("#eca_name1").focusout(function(){
        var letters = /^[A-Za-z '.-]+$/;
        var val = $(this).val();
        if (val != "") {
            if(!val.match(letters)) {
                $("#eca_name_error1").text("Name must contain only letters, space");   
            } else {
                $("#eca_name_error1").text("");   
            }
        } else {
            $("#eca_name_error1").text("Name is required");
        }
    });
    $("#eca_email1").focusout(function(){
        var letters = /^([\.a-zA-Z0-9_\-])+@([a-zA-Z0-9_\-])+(([a-zA-Z0-9_\-])*\.([a-zA-Z0-9_\-])+)+$/;
        var val = $(this).val();
        if (val != "") {
            if(!val.match(letters)) {
                $("#eca_email_error1").text("Email Address not valid");   
            } else {
                $("#eca_email_error1").text("");   
            }
        } else {
            $("#eca_email_error1").text("Email is required");
        }
    });
   $("#eca_mob1").focusout(function(){
        var letters = /^(?:(?:\+|0{0,2})91(\s*[\-]\s*)?|[0]?)?[6789]\d{9}$/;
       var val = $(this).val();
       if (val != "") {
           if(!val.match(letters)) {
               $("#eca_mob_error1").text("Phone number must contain only numbers and 10 digits");   
           } else {
            $("#eca_mob_error1").text("");   
           }
       } else {
            $("#eca_mob_error1").text("Phone number is required");
       }
   });
</script>
