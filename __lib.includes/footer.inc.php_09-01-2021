<!-- ---------------------------------------------- Function for Filtering  ----------------------------------------------------------- -->
<script type="text/javascript">
   function create_filter(){
       var amc_code      = $(".amc_code").val();
       var schm_type     = $(".schm_type").val();
       var sch_risk      = $(".sch_risk").val();
       var sch_fund_size = $(".sch_fund_size").val();
       //alert("Create Filter");
   
       //alert("amc_code:"+amc_code+"schm_type:"+schm_type+"sch_risk:"+sch_risk+"sch_fund_size:"+sch_fund_size);
       //filtering value of AMC
       var amc_code = [];
       $. each($("input[name='amc_code']:checked"), function(){
       amc_code. push($(this). val());
       });
       //console.log(JSON.stringify(amc_code));\
       //filtering value for Scheme Type
       var schm_type = [];
       $. each($(".schm_type:checked"), function(){
       schm_type. push($(this). val());
       //alert($(this). val());
       console.log(JSON.stringify(schm_type));
       });
       //console.log(JSON.stringify(schm_type));
       //filtering value for scheme Risk
       var sch_risk = [];
       $. each($("input[name='sch_risk']:checked"), function(){
       sch_risk. push($(this). val());
       });
       //console.log(JSON.stringify(sch_risk));
       //filtering value for Fund Size
       var sch_fund_size = [];
       $. each($("input[name='sch_fund_size']:checked"), function(){
       sch_fund_size. push($(this). val());
       });
      //calling ajax to getting filtering data.
       $.ajax({
           cache:false,
           url: "<?php echo $CONFIG->siteurl;?>__lib.ajax/ajax_response.php",
           type: 'POST',
           //contentType: "application/json; charset=utf-8",
           //dataType: "json",
           data: { "filter_search": "yes", "amc_code": amc_code, "schm_type": schm_type, "sch_risk": sch_risk, "sch_fund_size": sch_fund_size},
           success: function(response) {
           //alert(response);
           //console.log(response);
           
               if(response) 
               {
                   //console.log(response);
                   const obj = JSON.parse(response);
                   //console.log("Scheme_Name:"+obj.Scheme_Name);
                   //var  count = response.length;
                   //alert(count);
                   //console.log(response[0][0][Scheme_Name]);
                   //$('#all_pr_fetch').find('label').remove();   
                   var originalData = JSON.parse(response);
                   var parsedData = {};
                   for (var i = 0, l = originalData.length; i < l; i++) {
                       //parsedData[originalData[i].pk_recomend_id] = originalData[i].Scheme_Name;
                       console.log("Scheme_Name:"+originalData[i].Scheme_Name);
                       console.log("Length:"+originalData[i].length);
                       $.ajax({
                               cache:false,
                               url: "<?php echo $CONFIG->siteurl;?>__lib.ajax/ajax_response.php",
                               type: 'POST',
                               //contentType: "application/json; charset=utf-8",
                               //dataType: "json",
                               async:false,
                               data: { "get_nav_per": "yes", "ISIN":originalData[i].ISIN,"year":"1" },
                               success: function(res) {
                                //console.log("res"+res);
                                nav = res;
                                //var nav_per = response;
                                }
                              });
                        //console.log("NAV"+nav);
                       //console.log("I"+i);
                       var rem = i % 2;
                       if(rem == 0)
                       {
                           //console.log("HI");
                            $("#all_pr_fetch").prepend("<a class='col-md-12' href='single_product.html?i"+window.btoa(originalData[i].pk_nav_id)+"?"+window.btoa(nav)+"' style='color: #000000; padding-bottom: 5px;'> <div class='row card-body'> <div class='col-sm-8'> <p>  &nbsp;&nbsp;"+originalData[i].Scheme_Name+"<br><div class='col-sm-8' style='font-size: 13px; font-weight: 100; text-align: center; padding-right: 9%;'> . "+originalData[i].Scheme_Type+" <i class='fas fa-star' style='font-weight: 510; font-size: 10px;'></i> </div></p> </div><div class='col'>"+nav+"%<br><div class='col'> 1Y </div></div></div></a>");
                       }
                       else
                       {
                           //console.log("Bye");
                           $("#all_pr_fetch").prepend("<a class='col-md-12' href='single_product.html?"+window.btoa(originalData[i].pk_nav_id)+"?"+window.btoa(nav)+"' style='color: #000000; padding-bottom: 5px;'> <div class='row card-header'> <div class='col-sm-8'> <p>  &nbsp;&nbsp;"+originalData[i].Scheme_Name+"<br><div class='col-sm-8' style='font-size: 13px; font-weight: 100; text-align: center; padding-right: 9%;'> . "+originalData[i].Scheme_Type+" <i class='fas fa-star' style='font-weight: 510; font-size: 10px;'></i> </div></p> </div><div class='col'>"+nav+"%<br><div class='col'> 1Y </div></div></div></a>");
                       }
                   }
   
               }
              
           }            
       });
   }
</script>
