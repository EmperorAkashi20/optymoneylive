                       }
                       else
                       {
                           //console.log("Bye");
                        $("#all_pr_fetch").prepend("<a class='col-md-12' href='single_product.html?"+window.btoa(originalData[i].pk_nav_id)+"?"+window.btoa(nav)+"' style='color: #000000; padding-bottom: 5px;'> <div class='row card-header'> <div class='col-sm-8'> <p>  &nbsp;&nbsp;"+originalData[i].Scheme_Name+"<br><div class='col-sm-8' style='font-size: 13px; font-weight: 100; text-align: center; padding-right: 9%;'> . "+originalData[i].Scheme_Type+" <i class='fas fa-star' style='font-weight: 510; font-size: 10px;'></i> </div></p> </div><div class='col'>"+nav+"%<br><div class='col'> 1Y </div></div></div></a>");
                       }
                   }
   
               }
              
           }            
         });
   
       /*------------------------------------------------------------- Fetch grpah data for Single page ----------------------------------------------------------------*/
   
       var sch_code = $("#sch_code").val();
       if( $('#sch_code').val() == '0' || $('#sch_code').val() == '' || $('#sch_code').val() == 'undefined' || $('#sch_code').val() == null )
       {
         //alert(sch_code);
