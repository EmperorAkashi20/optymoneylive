<script type="text/javascript">
    $("#hire_now").click(function(e){
        event.preventDefault(); 
        var temp = 0;
        var letters = /^[A-Za-z '.-]+$/;
        var val = $("#eca_name").val();
        if (val != "") {
            if(!val.match(letters)) {
                $("#eca_name_error").text("Name must contain only letters, space");   
                temp = 1;
            }
            else {
                temp = 0;
                $("#eca_name_error").text("");
            }
        } else {
            $("#eca_name_error").text("Name is required");
            temp = 1;
        }
        if(temp==0) {
            var letters = /^([\.a-zA-Z0-9_\-])+@([a-zA-Z0-9_\-])+(([a-zA-Z0-9_\-])*\.([a-zA-Z0-9_\-])+)+$/;
            var val = $("#eca_email").val();
            if (val != "") {
                if(!val.match(letters)) {
                    $("#eca_email_error").text("Email Address not valid");   
                    temp = 1;
                } else {
                    temp = 0;
                    $("#eca_email_error").text("");
                }
            } else {
                $("#eca_email_error").text("Email is required");
                temp = 1;
            }
        }
        if(temp==0) {
            var letters = /^(?:(?:\+|0{0,2})91(\s*[\-]\s*)?|[0]?)?[6789]\d{9}$/;
            var val = $("#eca_mob").val();
            if (val != "") {
                if(!val.match(letters)) {
                    $("#eca_mob_error").text("Enter Valid Phone Number");   
                    temp = 1;
                } else {
                    temp = 0;
                    $("#eca_mob_error").text("");
                }
            } else {
                    $("#eca_mob_error").text("Phone number is required");
                    temp = 1;
            }
        }
        console.log(temp);
        if(temp==0) {
            alert("Thank you for contacting us. We will contact with you shortly.");
        } else {

        }
       
   });
</script>
