<script type="text/javascript">
   $(".rmv_sip").click(function(){
       //alert("HI");
       $("#sip_date").remove(required);
   });
   $(".add_sip").click(function(){
       //alert("HI");
       $("#sip_date").append(required);
   });
</script>
<script type="text/javascript">
   $("#sip_date").change(function(){
       var val = $(this).val();
       if(val== "1" || val== "21") 
       { 
         $("#dt_typ").text("st");
       }
       else if(val == "2" || val == "22")
       { 
         $("#dt_typ").text("nd");
       } 
       else if(val == "3" || val == "23")
       {
           $("#dt_typ").text("rd");
       }
       else
       {
           $("#dt_typ").text("th");
       }
   });
</script>
